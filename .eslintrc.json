{
  "extends": ["@rocketseat/eslint-config/node"],
  "rules": {
    "camelcase": "off",
    "no-useless-constructor": "off"
  },
  "comma-dangle": [
    "error",
    {
      "arrays": "never",
      "objects": "never",
      "imports": "never",
      "exports": "never",
      "functions": "never"
    }
  ]
}
